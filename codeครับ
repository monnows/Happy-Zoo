#define BLYNK_PRINT Serial    // Comment this out to disable prints and save space
#include <ESP8266WiFi.h>
#include <BlynkSimpleEsp8266.h>
#include <Servo.h>
Servo servo;

char auth[] = "bpNIIg8y57uH5GwTs0muWAqUMCuPgdDK";
char ssid[] = "Thekiller_2.4G";
char pass[] = "non172352";
void setup(){
  Serial.begin(9600);
  servo.attach(5);
  servo.write(22);
  Blynk.begin(auth, ssid, pass);
  
}
 BLYNK_WRITE(V1){
    servo.write(90);
    delay(100);
    servo.write(22);
    delay(100);
    }
void loop(){

    Blynk.run();
}
